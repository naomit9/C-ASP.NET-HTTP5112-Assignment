
@model SchoolProject.Models.Course

@{
    ViewBag.Title = "Show";
}

<a class="navlink" href="/Course/List">Back to Courses</a>

<div id="containershow">
    <h2>
        @Model.ClassName
    </h2>
    <p>
        Course Code: @Model.ClassCode
    </p>
    <p>
        Start Date: @Model.StartDate
    </p>
    <p>
        End Date: @Model.FinishDate
    </p>
</div>

